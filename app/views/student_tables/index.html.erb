<p id="notice"><%= notice %></p>

<h1>Student Tables</h1>

<table>
  <thead>
    <tr>
      <th>Branch</th>
      <th>Name</th>
      <th>Contactnumber</th>
      <th>Gender</th>
      <th>Cgpa</th>
      <th>Permanentaddress</th>
      <th>Semester</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @student_tables.each do |student_table| %>
      <tr>
        <td><%= student_table.branch %></td>
        <td><%= student_table.name %></td>
        <td><%= student_table.contactNumber %></td>
        <td><%= student_table.gender %></td>
        <td><%= student_table.cgpa %></td>
        <td><%= student_table.permanentAddress %></td>
        <td><%= student_table.semester %></td>
        <td><%= student_table.user_id %></td>
        <td><%= link_to 'Show', student_table %></td>
        <td><%= link_to 'Edit', edit_student_table_path(student_table) %></td>
        <td><%= link_to 'Destroy', student_table, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student Table', new_student_table_path %>
